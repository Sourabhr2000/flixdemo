<%= form_with(model: [@movie, @review], local: true) do |f| %>

  <%= render "movies/errors", object: @review %>

  <%= f.label :stars %>
  <%= f.select :stars, [1,2,3,4,5], prompt: "Pick one" %>

  <%= f.label :comment %>
  <%= f.text_area :comment, placeholder: "comment here.." %>

  <%= f.submit "Post Review" %>
<% end %>